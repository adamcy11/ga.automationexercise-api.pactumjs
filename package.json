{
  "name": "ga.automationexercise-api.pactumjs",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "mocha \"tests/**/*.spec.js\" --file src/hooks/mocha.global.js --reporter mochawesome --timeout 30000",
    "test:junit": "mocha \"tests/**/*.spec.js\" --file src/hooks/mocha.global.js --reporter mocha-junit-reporter --reporter-options mochaFile=reports/junit/results.xml --timeout 30000",
    "test:ci": "npm run lint && mocha \"tests/**/*.spec.js\" --file src/hooks/mocha.global.js --reporter mocha-junit-reporter --reporter-options mochaFile=reports/test-results.xml",
    "lint": "eslint \"**/*.js\"",
    "report:open": "npx mochawesome-merge ./mochawesome-report/*.json | npx marge --reportDir reports/html --inline true"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "joi": "^17.12.0",
    "pactum": "^3.6.0"
  },
  "devDependencies": {
    "chai": "^4.5.0",
    "eslint": "^9.11.1",
    "mocha": "^10.7.0",
    "mocha-junit-reporter": "^2.2.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0"
  }
}