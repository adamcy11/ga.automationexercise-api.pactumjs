{
  "name": "ga.automationexercise-api.pactumjs",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "mocha \"tests/**/*.spec.js\" --file src/hooks/mocha.global.js --reporter mochawesome --timeout 30000",
    "test:health": "mocha \"tests/health/**/*.spec.js\" --file src/hooks/mocha.global.js --reporter mochawesome --timeout 10000",
    "test:contract": "mocha \"tests/contract/**/*.spec.js\" --file src/hooks/mocha.global.js --reporter mochawesome --timeout 30000",
    "test:users": "mocha \"tests/e2e/users/**/*.spec.js\" --file src/hooks/mocha.global.js --reporter mochawesome --timeout 40000",
    "test:products": "mocha \"tests/e2e/products/**/*.spec.js\" --file src/hooks/mocha.global.js --reporter mochawesome --timeout 40000",
    "test:ci": "mocha \"tests/**/*.spec.js\" --file src/hooks/mocha.global.js --reporter mocha-multi-reporters --reporter-options configFile=mocha-multi.json --timeout 60000",
    "report:open": "start mochawesome-report/mochawesome.html"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "joi": "^17.12.0",
    "pactum": "^3.6.0"
  },
  "devDependencies": {
  "chai": "^4.5.0",
  "eslint": "^9.11.1",
  "mocha": "^10.7.0",
  "mocha-junit-reporter": "^2.2.0",
  "mocha-multi-reporters": "^1.5.1",
  "mochawesome": "^7.1.3",
  "mochawesome-merge": "^4.3.0",
  "mochawesome-report-generator": "^6.2.0"
}
}